{
	"name": "@mappa-ai/mappa-node",
	"module": "src/index.ts",
	"type": "module",
	"license": "MIT",
	"files": [
		"dist/**"
	],
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"default": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.cjs",
				"default": "./dist/index.cjs"
			}
		}
	},
	"devEngines": {
		"runtime": {
			"name": "bun",
			"version": ">=1.3.0"
		},
		"packageManager": {
			"name": "bun"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mappa-ai/mappa-node.git"
	},
	"scripts": {
		"build": "tsdown",
		"type-check": "tsc --noEmit",
		"test": "bun test",
		"check": "biome check",
		"check:fix": "biome check --fix"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@types/bun": "latest",
		"tsdown": "^0.19.0",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"zod": "^4.3.5"
	}
}
